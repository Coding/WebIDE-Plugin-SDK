{
  "name": "codingSDK",
  "version": "1.1.0-alpha.03",
  "description": "WebIDE-Plugin-SDK for Coding WebIDE",
  "repository": {
    "url": "git@github.com:Coding/WebIDE-Plugin-SDK.git"
  },
  "license": "ISC",
  "author": "candy <candy.zhengxq@gmail.com>",
  "main": "server.js",
  "bin": {
    "codingIde": ".script/build.sh"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development nodemon --watch package.json --watch server.js --exec babel-node server.js",
    "build": "yarn run clean && NODE_ENV=production webpack --config webpack.production.config.js --progress --profile --colors && yarn run generateManifest",
    "prebuild": "yarn && sh .script/prebuild.sh",
    "generateManifest": "node .script/buildManifest.js",
    "getPackage": "yarn && sh .script/getPackage.sh",
    "linkSDK": "ln -s $(pwd) ${PROJECT}/node_modules/codingSDK",
    "server": "node server.prod.js"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.2.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.5.0",
    "webpack": "^2.2.1",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.17.0"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.1.8",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.17.2",
    "cors": "^2.8.1",
    "fs-promise": "^1.0.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "socket.io": "^2.0.3",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "npm": "^3.0.0"
  }
}
